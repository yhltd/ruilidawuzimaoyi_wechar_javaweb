<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.myboot.mapper.PeiZhiShuiLvMapper">

    <update id="update">
        UPDATE peizhi_shuilv set shuilv=#{shuilv},zhuangtai=#{zhuangtai},lingshou=#{lingshou}, pifa=#{pifa},
        dakehu=#{dakehu}
        WHERE id = #{id}
    </update>

    <select id="getAll" resultType="com.myboot.pojo.PeiZhiShuiLv">
        <![CDATA[
            select * from peizhi_shuilv
        ]]>
    </select>

</mapper>